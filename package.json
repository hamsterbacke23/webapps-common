{
  "name": "webapps-common",
  "version": "0.0.0",
  "description": "Shared files for web projects. See README for details.",
  "license": "UNLICENSED",
  "author": "KNIME AG, Zurich, Switzerland",
  "scripts": {
    "preinstall": "node ./install-subDependencies",
    "generate-css": "node ./ui/colors/generateCSS.js",
    "postinstall": "npm run generate-css",
    "demo": "npm --prefix demo/ install && npm run --prefix demo/ dev",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "test:unit": "jest",
    "coverage": "jest --coverage",
    "sendcoverage": "cross-var sonar-scanner -Dsonar.login=\"$SONAR_LOGIN\" -Dsonar.password=\"$SONAR_PASSWORD\"",
    "opensourcecredits": "npm run --prefix buildtools/ opensourcecredits"
  },
  "dependencies": {
    "filesize": "6.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.5.5",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "consola": "2.10.1",
    "cross-var": "1.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.3",
    "jest": "24.9.0",
    "jest-junit": "6.4.0",
    "jest-transform-stub": "2.0.0",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "vue": "2.6.11",
    "vue-jest": "3.0.4",
    "vue-template-compiler": "2.6.11"
  },
  "peerDependencies": {
    "consola": "*",
    "eslint": "5.x - 7.x",
    "normalize.css": "*",
    "postcss-calc": "*",
    "postcss-css-variables": "*",
    "postcss-import": "*",
    "postcss-preset-env": "6.x",
    "postcss-url": "*",
    "vue-clickaway": "2.2.2",
    "stylelint": "10.x - 13.x",
    "stylelint-config-standard": "18.x - 20.x",
    "typeface-roboto": "*",
    "typeface-roboto-mono": "*"
  },
  "private": true
}
